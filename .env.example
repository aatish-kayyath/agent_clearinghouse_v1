# ============================================================
# Agentic Clearinghouse - Environment Configuration
# ============================================================
# Copy this file to .env and fill in your values:
#   cp .env.example .env
# ============================================================

# --- Database (PostgreSQL) ---
DATABASE_URL=postgresql+asyncpg://clearinghouse:clearinghouse_dev@localhost:5432/agentic_clearinghouse

# --- Redis ---
REDIS_URL=redis://localhost:6379/0

# --- Application ---
APP_ENV=development
APP_DEBUG=true
APP_LOG_LEVEL=DEBUG

# --- E2B Sandbox (Step 2) ---
E2B_API_KEY=

# --- LLM / LiteLLM (Step 2) ---
OPENAI_API_KEY=
LITELLM_MODEL=gpt-4o
LITELLM_FALLBACK_MODELS=gpt-4o-mini

# --- Coinbase Developer Platform (Step 4) ---
CDP_API_KEY_ID=
CDP_API_KEY_SECRET=
CDP_WALLET_SECRET=
CDP_NETWORK_ID=base-sepolia

# --- MCP Server ---
MCP_TRANSPORT=streamable-http
